<div>
  <%= form_for @clothe do |f| %>
  <%= f.label :name, "Name" %>
  <%= f.text_field :name %>
  <%= f.label :description, "Description" %>
  <%= f.text_area :description %>
  <%= f.label :price, "Price" %>
  <%= f.text_field :price %>
  <%= f.submit "Update", :class => "btn btn-primary" %>
<% end %>
</div>